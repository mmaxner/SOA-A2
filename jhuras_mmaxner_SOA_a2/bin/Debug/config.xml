<services>
  <service>
    <name>Calculator</name>
    <url>http://www.dneonline.com/calculator.asmx</url>
	<namespace>http://tempuri.org/</namespace>
    <action>
      <name>Add</name>
        <parameter>
          <type>int</type>
          <dataName>intA</dataName>
          <uiName>1st Number</uiName>
        </parameter>
        <parameter>
          <type>int</type>
          <dataName>intB</dataName>
          <uiName>2nd Number</uiName>
        </parameter>
    </action>
    <action>
      <name>Subtract</name>
      <parameter>
        <type>int</type>
        <dataName>intA</dataName>
        <uiName>1st Number</uiName>
      </parameter>
      <parameter>
        <type>int</type>
        <dataName>intB</dataName>
        <uiName>2nd Number</uiName>
      </parameter>
    </action>
    <action>
      <name>Multiply</name>
      <parameter>
        <type>int</type>
        <dataName>intA</dataName>
        <uiName>1st Number</uiName>
      </parameter>
      <parameter>
        <type>int</type>
        <dataName>intB</dataName>
        <uiName>2nd Number</uiName>
      </parameter>
    </action>
    <action>
      <name>Divide</name>
      <parameter>
        <type>int</type>
        <dataName>intA</dataName>
        <uiName>1st Number</uiName>
      </parameter>
      <parameter>
        <type>int</type>
        <dataName>intB</dataName>
        <uiName>2nd Number</uiName>
		<custom_validation>
			<not_equal>0</not_equal>
		</custom_validation>
      </parameter>
    </action>
  </service>

  <service>
    <name>Currency Converter</name>
    <url>http://currencyconverter.kowabunga.net/converter.asmx</url>
	<namespace>http://tempuri.org/</namespace>
    <action>
      <name>GetCurrencyRate</name>
      <parameter>
        <type>list</type>
        <listsource>
          <servicename>GetCurrencies</servicename>
          <displaymember>string</displaymember>
          <datamember>string</datamember>
        </listsource>
        <dataName>Currency</dataName>
        <uiName>Currency</uiName>
      </parameter>
      <parameter>
        <type>date</type>
        <dataName>RateDate</dataName>
        <uiName>Date</uiName>
      </parameter>
    </action>
    <action>
      <name>GetConversionAmount</name>
      <parameter>
        <type>list</type>
        <listsource>
          <servicename>GetCurrencies</servicename>
          <displaymember>string</displaymember>
          <datamember>string</datamember>
        </listsource>
        <dataName>CurrencyFrom</dataName>
        <uiName>Base Currency</uiName>
      </parameter>
      <parameter>
        <type>list</type>
        <listsource>
          <servicename>GetCurrencies</servicename>
          <displaymember>string</displaymember>
          <datamember>string</datamember>
        </listsource>
        <dataName>CurrencyTo</dataName>
        <uiName>Target Currency</uiName>
      </parameter>
      <parameter>
        <type>date</type>
        <dataName>RateDate</dataName>
        <uiName>Date</uiName>
      </parameter>
      <parameter>
        <type>decimal</type>
        <dataName>Amount</dataName>
        <uiName>Amount</uiName>
      </parameter>
    </action>
   
  </service>
	<service>
